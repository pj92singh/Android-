<resources>
<!-- 05/18/ -->
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->

        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <!-- 2nd Dark theme. -->
    <style name="AppThemedark" parent="Theme.AppCompat.Light.DarkActionBar" >
        <item name="android:colorPrimary">@android:color/black</item>
        <item name="android:colorPrimaryDark">@android:color/darker_gray</item>
        <item name="android:colorBackground">@android:color/background_dark</item>
        <item name="android:textColorPrimary">@android:color/background_light</item>
        <item name="android:colorForeground">@android:color/darker_gray</item>
    </style>

</resources>
